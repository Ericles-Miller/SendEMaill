generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Users {
  id        String    @id @default(uuid())
  name      String
  email     String
  password  String
  createdAt DateTime  @default(now())
  updatedAt DateTime? @default(now())
  enable    Boolean

  @@map("users")
}

